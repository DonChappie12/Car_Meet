<h1>Get Back to the Action</h1>
<h3>Login</h3>
@model User
<form asp-action="LoginIn" asp-controller="Home" method="POST">
    <h3>@ViewBag.error</h3>
    <p>Email: <input asp-for="Email"></p>
    <p>Password: <input asp-for="Password"></p>
    <input type="submit" value="Login">
</form>

<a href="/">Not a member? Then what are you doing here?</a>